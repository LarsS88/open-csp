{
  "require": {
    "mediawiki/admin-Links": "dev-REL1_39",
    "mediawiki/bootstrap": "4.5.0",
    "mediawiki/chameleon-skin": "^4.2",
    "mediawiki/data-transfer": "dev-REL1_39",
    "mediawiki/display-title": "dev-REL1_39",
    "mediawiki/my-variables": "dev-REL1_39",
    "mediawiki/replace-text": "dev-REL1_39",
    "mediawiki/semantic-extra-special-properties": "3.0.4",
    "mediawiki/semantic-media-wiki": "^4.1.1",
    "mediawiki/semantic-result-formats": "4.0.2",
    "mediawiki/semantic-scribunto": "2.2.0",
    "mediawiki/url-get-parameters": "dev-REL1_39",
    "mediawiki/user-functions": "dev-REL1_39",
    "mediawiki/widgets": "dev-REL1_39",
    "elasticsearch/elasticsearch": "^6.8",
    "open-csp/flex-form": "dev-REL1_39",
    "open-csp/page-sync": "dev-REL1_39",
    "open-csp/w-s-slots": "^4.0",
    "open-csp/w-s-spaces": "^2.2",
    "open-csp/wiki-search": "dev-REL1_39",
    "wikibase-solutions/array-functions": "v1.4.2",
    "wikibase-solutions/c-s-p-resources": "v2.0.2",
    "wikibase-solutions/wiki-search-front": "v2.0.4"
  },
  "repositories": {
    "adminlinks": {
      "package": {
        "name": "mediawiki/admin-links",
        "type": "mediawiki-extension",
        "version": "dev-REL1_39",
        "source": {
          "reference": "REL1_39",
          "type": "git",
          "url": "https://github.com/wikimedia/mediawiki-extensions-AdminLinks.git"
        }
      },
      "type": "package"
    },
    "displaytitle": {
      "package": {
        "name": "mediawiki/display-title",
        "type": "mediawiki-extension",
        "version": "dev-REL1_39",
        "source": {
          "reference": "REL1_39",
          "type": "git",
          "url": "https://github.com/wikimedia/mediawiki-extensions-DisplayTitle.git"
        }
      },
      "type": "package"
    },
    "wikisearch": {
	    "package": {
		    "name": "open-csp/wiki-search",
		    "type": "mediawiki-extension",
		    "version": "dev-REL1_39",
		    "source": {
			    "reference": "139-compat",
			    "type": "git",
			    "url": "https://github.com/Open-CSP/WikiSearch.git"
		    }
	    },
	    "type": "package"
    },
    "myvariables": {
      "package": {
        "name": "mediawiki/my-variables",
        "type": "mediawiki-extension",
        "version": "dev-REL1_39",
        "source": {
          "reference": "REL1_39",
          "type": "git",
          "url": "https://github.com/wikimedia/mediawiki-extensions-MyVariables.git"
        }
      },
      "type": "package"
    },
    "replacetext": {
      "package": {
        "name": "mediawiki/replace-text",
        "type": "mediawiki-extension",
        "version": "dev-REL1_39",
        "source": {
          "reference": "REL1_39",
          "type": "git",
          "url": "https://github.com/wikimedia/mediawiki-extensions-ReplaceText.git"
        }
      },
      "type": "package"
    },
    "urlgetparameters": {
      "package": {
        "name": "mediawiki/url-get-parameters",
        "type": "mediawiki-extension",
        "version": "dev-REL1_39",
        "source": {
          "reference": "REL1_39",
          "type": "git",
          "url": "https://github.com/wikimedia/mediawiki-extensions-UrlGetParameters.git"
        }
      },
      "type": "package"
    },
    "widgets": {
      "package": {
        "name": "mediawiki/widgets",
        "type": "mediawiki-extension",
        "version": "dev-REL1_39",
        "source": {
          "reference": "REL1_39",
          "type": "git",
          "url": "https://github.com/wikimedia/mediawiki-extensions-Widgets.git"
        }
      },
      "type": "package"
    }
  },
  "extra": {
    "merge-plugin": {
      "include": [
        "./extensions/OATHAuth/composer.json",
        "./extensions/WikiSearch/composer.json",
        "./extensions/Widgets/composer.json"
      ],
      "merge-replace": false
    }
  },
  "autoload": {
    "classmap": [
      "skin/components"
    ]
  }
}
